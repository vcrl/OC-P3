for wall in walls:
            current_x = self.rect.x
            current_y = self.rect.y

            if wall.rect.x == self.rect.x + dx and wall.rect.y == self.rect.y:
                self.rect.x = current_x
                self.rect.y = current_y
            else:
                self.rect.x += dx * TILESIZE
                self.rect.y += dy * TILESIZE